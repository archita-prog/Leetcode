lass Solution {
public:
    int findMaxLength(vector<int>& nums) {
        int count =0;
        int max_len =0;
        unordered_map<int, int> f;
        f[0] = -1;
        for(int i =0; i < nums.size(); i++){
            nums[i] == 0 ? count-- : count++;
            if(f.count(count)){
                max_len = max(max_len, i- f[count]);
            }
            else{
                f[count] = i;
            }
        }
        return max_len;
        
    }
};
