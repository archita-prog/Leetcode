class Solution {
public:
    vector<int> findDisappearedNumbers(vector<int>& nums) {
        int n= nums.size();
        vector<bool> present(n+1, false) ;
        for(int num : nums){
            present[num] = true;

        }
        vector<int> res;
        for(int i =1; i<= n; ++i){
            if(!present[i]){
                res.push_back(i);
            }

        }
        return res;
    }
};
