class Solution {
public:
    bool rotateString(string s, string goal) {
        int j = s.length();
        if(s == goal) return true;
        for(int i = 0; i< j;i++){
            string rotated = s.substr(i) + s.substr(0,i) ;
            if(rotated == goal) return true;

        }
        return false;
        
    }
};
