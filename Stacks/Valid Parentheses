class Solution {
public:
    bool isValid(string s) {
        int n=s.length();
        if(n%2!=0){
            return false;
        }
        char openParen[n];
        int index=0;
        for(char ch:s){
            if(ch=='(' || ch=='{'||ch=='['){
                openParen[index++]=ch;
            }
            else{
                if(index==0) return false;
                char lastOpen=openParen[--index];
                if((ch==')' && lastOpen!='(') ||
                  (ch=='}' && lastOpen!='{') || 
                  (ch==']' && lastOpen!='[')){
                    return false;
                }
            }
        }
        return index==0;
        
    }
};
